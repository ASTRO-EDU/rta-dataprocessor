<?xml version="1.0" encoding="utf-8"?>
<dl2model>
	<group name="/dl2">
		<dataset name="eventlist" type="compound" rank="1">
			<fields>
				<field name="n_waveform" type="float32"/>		<!-- DL0 waveform index -->
				<field name="mult" type="float32"/>				<!-- Mult value in case it's the same waveform -->
				<field name="tstart" type="float64"/>			<!-- tstart DL0 waveform -->
				<field name="index_peak" type="float32"/>		<!-- DL0 index of the detected peak -->
				<field name="peak" type="float32"/>				<!-- Peak value -->
				<field name="integral1" type="float32"/>		<!-- Area of the waveform predicted with the ML model -->
				<field name="integral2" type="float32"/>
				<field name="integral3" type="float32"/>
				<field name="halflife" type="float32"/>
				<field name="temp" type="float32"/>
			</fields>
		</dataset>
	</group>
</dl2model>